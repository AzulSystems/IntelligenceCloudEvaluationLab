APIKEY=<SET THIS>
ENDPOINT=https://azul.public-api.crs-prod.azul.com/public/report/codeInventory
APPENV=<SET THIS>
REPORTNAME=<SET THIS>

curl -s -X 'POST' \
  ''$ENDPOINT'' \
  -H 'accept: application/json' \
  -H 'Content-Type: application/json' \
  -H 'x-api-key: '$APIKEY'' \
  -d '{
  "filter":[
	"AppEnv CONTAINS '\''$APPENV'\''"
],
  "reportName":"'$REPORTNAME'"
}'